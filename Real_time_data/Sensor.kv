#:kivy 1.0.9
#: import Window kivy.core.window.Window
<SensorMonitor>:
    size: 1000, 1000
    canvas.before:
        Rectangle:
            pos: self.pos
            size: 1600, 1200
            source: './city.jpeg'
<Sensor1>:
    canvas:
        Color: 
            rgb: self.colour
        Ellipse:
            id: poi1
            size: 40, 40
            pos: self.x + 130, self.y + 310

